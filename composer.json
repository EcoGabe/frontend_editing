{
    "name": "pixelant/frontend-editingg",
    "type": "typo3-cms-extension",
    "description": "Enable editors to work with the content in the most intuitive way possible",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "feediting"],
    "authors": [
        {
            "name": "Jozef Spisiak",
            "email": "jozef@pixelant.se",
            "role": "Developer"
        },
        {
            "name": "Phat Hoang",
            "email": "phat@pixelant.se",
            "role": "Developer"
        },
        {
            "name": "Mattias Nilsson",
            "email": "mattias@pixelant.se",
            "role": "Developer"
        }
    ],
    "version": "0.0.1",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "typo3/cms": "^7.6",
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~4.8.0",
        "mikey179/vfsStream": "~1.6.0"
    },
    "replace": {
        "frontend_editing": "self.version"
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\CMS\\FrontendEditing\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\CMS\\FrontendEditing\\Tests\\": "Tests/",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
            "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/frontend_editing ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/frontend_editing"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}